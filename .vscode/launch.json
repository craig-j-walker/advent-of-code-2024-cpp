    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/day3/main",
            "args": [
                "arg1",
                "arg2"
            ],
            "environment": [
                {
                    "name": "config",
                    "value": "Debug"
                }
            ],
            "cwd": "${workspaceFolder}",
            "externalConsole": true,
            "stopAtEntry": false,
            "MIMode": "gdb",
            "miDebuggerPath": "",
            "setupCommands": [
                {
                    "text": "target-run",
                    "description": "run target",
                    "ignoreFailures": false
                }
            ],
            "logging": {
                "moduleLoad": false,
                "trace": true
            }
        }
    ]
}